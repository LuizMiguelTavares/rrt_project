<launch>
    <node pkg="rosbag" type="record" name="rosbag_record" 
          args="-O $(find rrt_project)/rosbag/simulations/simple_rrt_potential/zig_zag/my_bagfile.bag --all" output="screen">
        <param name="use_sim_time" value="true"/>
    </node>

    <include file="$(find rrt_project)/launch/simulation/different_rrt_methods/simple_rrt_simulation_potential.launch">
        <arg name="x_goal" value="2.66"/>
        <arg name="y_goal" value="-1"/>
    </include>
</launch>