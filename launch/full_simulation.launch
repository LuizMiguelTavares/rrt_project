<launch>

    <arg name="x_goal" default="5.5" />
    <arg name="y_goal" default="5.5" />

    <include file="$(find rrt_project)/launch/robot_and_map_setup.launch">
        <arg name="paused" value="false" />
        <arg name="use_sim_time" value="true" />
        <arg name="gui" value="true" />
        <arg name="headless" value="false" />
        <arg name="debug" value="false" />
    </include>

    <include file="$(find rrt_project)/launch/rrt_potential_launch.launch">
        <arg name="new_robot_pose_topic" value="/amcl_pose" />
        <arg name="new_robot_pose_message" value="PoseWithCovarianceStamped" />
        <arg name="new_robot_control_topic" value="/cmd_vel" />
        <arg name="new_robot_control_message" value="Twist" />
        <arg name="x_goal" value="$(arg x_goal)" />
        <arg name="y_goal" value="$(arg y_goal)" />
    </include>
</launch>